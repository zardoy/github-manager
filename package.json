{
    "name": "github-manager",
    "publisher": "zardoy",
    "displayName": "GitHub Manager",
    "description": "Quickly switch between cloned GitHub repos",
    "version": "0.0.1",
    "files": [
        "out"
    ],
    "categories": [
        "Other"
    ],
    "keywords": [
        "git",
        "folder",
        "local",
        "switch",
        "open"
    ],
    "activationEvents": [
        "onCommand:github-manager.open-github-repos",
        "onCommand:github-manager.open-non-git-dirs",
        "onCommand:github-manager.open-non-remote-repos"
    ],
    "contributes": {
        "commands": [
            {
                "category": "GitHub Manager",
                "command": "github-manager.open-github-repos",
                "title": "Open Cloned GitHub Repository"
            },
            {
                "category": "GitHub Manager",
                "command": "github-manager.open-non-git-dirs",
                "title": "Open Non-Git Directory"
            },
            {
                "category": "GitHub Manager",
                "command": "github-manager.open-non-remote-repos",
                "title": "Open Non-Remote Repository"
            }
        ]
    },
    "scripts": {
        "watch": "node esbuild.mjs dev",
        "compile": "node esbuild.mjs prod",
        "pretest": "yarn run compile && yarn run lint",
        "test": "node ./out/test/runTest.js",
        "vscode:prepublish": "tsc && yarn update-package-json && yarn compile",
        "update-package-json": "ts-node scripts/formPackageJson.ts",
        "lint": "xo src",
        "lint:fix": "xo src --fix"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/lodash": "^4.14.171",
        "@types/mocha": "^9.0.0",
        "@types/node": "16.4.3",
        "@types/react": "^17.0.14",
        "@types/react-dom": "^17.0.9",
        "@types/vscode": "^1.58.0",
        "@zardoy/xo-config": "^0.0.2",
        "esbuild": "^0.12.17",
        "glob": "^7.1.7",
        "mocha": "^9.0.2",
        "modify-json-file": "^1.1.0",
        "type-fest": "^1.2.2",
        "typescript": "^4.3.5",
        "vscode-test": "^1.6.1",
        "xo": "^0.42.0"
    },
    "dependencies": {
        "@octokit/rest": "^18.6.7",
        "@zardoy/tsconfig": "^1.1.0",
        "acorn": "^8.4.1",
        "acorn-walk": "^8.1.1",
        "find-up": "^5.0.0",
        "git-remote-origin-url": "^3.1.0",
        "lodash": "^4.17.21",
        "nanoid": "^3.1.23",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "read-pkg": "^6.0.0"
    },
    "engines": {
        "vscode": "^1.58.1"
    },
    "license": "MIT",
    "main": "out.js",
    "qna": false,
    "repository": "https://github.com/zardoy/github-manager"
}
