{
    "name": "github-manager",
    "displayName": "GitHub Manager",
    "publisher": "zardoy",
    "description": "Fastest way to switch between cloned GitHub repos",
    "icon": "resources/icon.png",
    "version": "0.0.1",
    "categories": [
        "Other"
    ],
    "keywords": [
        "offline",
        "folder",
        "local",
        "switch",
        "open"
    ],
    "activationEvents": [
        "onCommands"
    ],
    "contributes": {
        "commands": [
            {
                "command": "openClonedGithubRepository",
                "title": "Open Cloned GitHub Repository..."
            },
            {
                "command": "openGithubRepository",
                "title": "Open GitHub Repository..."
            },
            {
                "command": "openForkedGithubRepository",
                "title": "Open Forked GitHub Repository..."
            },
            {
                "command": "openClonedForkedGithubRepository",
                "title": "Open Cloned Forked GitHub Repository..."
            },
            {
                "command": "openNonGitDirectory",
                "title": "Open Non-Git Directory..."
            },
            {
                "command": "openNonRemoteRepository",
                "title": "Open Non-Remote Repository..."
            },
            {
                "command": "openEverything",
                "title": "Open Non-Git Directory Or Repository (everything)..."
            },
            {
                "command": "openAtGithub",
                "title": "Open At GitHub"
            }
        ],
        "configuration": {
            "title": "GitHub Manager",
            "properties": {
                "whereToOpen": {
                    "type": "string",
                    "enum": [
                        "newWindowIfNotEmpty",
                        "alwaysSameWindow",
                        "ask(after)",
                        "ask(before)"
                    ],
                    "default": "newWindowIfNotEmpty"
                },
                "boostRecentlyOpened": {
                    "type": "boolean",
                    "description": "Boost to top repos that were recently opened. It syncs and applies to all commands.\nBut note we don't have access to VSCode's recently opened menu",
                    "default": true
                },
                "ignore.users": {
                    "type": "array",
                    "description": "From which owners don't display GitHub repos",
                    "items": {
                        "type": "string"
                    },
                    "uniqueItems": true,
                    "default": []
                },
                "ignore.dirNameRegex": {
                    "type": "string",
                    "description": "Regexp to filter out dirs. Applies to all commands. Can be either string or regex with flags.\nExamples:\n - test \n - /test/g\n - dir1|dir2",
                    "default": ""
                },
                "showFolderNames": {
                    "type": "string",
                    "description": "Whether to show folder names next to repo slug when openining cloned GitHub repository",
                    "enum": [
                        "never",
                        "onDuplicates",
                        "always"
                    ],
                    "enumDescriptions": [
                        "Never show folder names",
                        "Show folder names only on duplicate entries (same owner/repo)",
                        "Always show folder names"
                    ],
                    "default": "onDuplicates"
                },
                "enableAuthentication": {
                    "type": "boolean",
                    "description": "Whether to enable inegration with your GitHub accout via authentication\nDisable to work with cloned repos only",
                    "default": true
                },
                "forkDetectionMethod": {
                    "type": "string",
                    "description": "How to detect clonned repo forks",
                    "enum": [
                        "alwaysOnline",
                        "fallback",
                        "upstreamRemote"
                    ],
                    "markdownEnumDescriptions": [
                        "Not available when #enableAuthentication# is `false`. Requires authentication and internet connection. Most accurate",
                        "Use `alwaysOnline` and `upstreamRemote` when no internet connection",
                        "Count repo as fork if it has `remote` upstream in config"
                    ],
                    "default": "fallback"
                },
                "onlineRepos.showArchived": {
                    "type": "boolean",
                    "description": "Whether to show archived non-cloned repos",
                    "default": false
                },
                "onlineRepos.sortBy": {
                    "type": "string",
                    "description": "How to sort repos when displaying with not-cloned, other cloned repos are sorted according to #sortClonedBy# setting",
                    "enum": [
                        "lastUpdated",
                        "lastPushed",
                        "fullName",
                        "created"
                    ],
                    "enumDescriptions": [
                        "Sort by count of owner's repositories",
                        "Sort by count last modified of directory of repository",
                        "Sort by recently opened via this extension. We don't have access to VSCode's recently opened"
                    ],
                    "default": "recentlyOpened"
                },
                "onlineRepos.reposType": {
                    "type": "string",
                    "enum": [
                        "all",
                        "owner",
                        "public",
                        "private",
                        "member"
                    ],
                    "description": "Which repository types to show",
                    "default": "all"
                },
                "onlineRepos.clonedDirFormat": {
                    "type": "string",
                    "enum": [
                        "repoName",
                        "repoOwner_repoName"
                    ],
                    "description": "Format to use for cloning directory name",
                    "default": "repoName"
                }
            }
        }
    },
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        },
        "virtualWorkspaces": true
    },
    "scripts": {
        "start": "vscode-framework start",
        "build": "vscode-framework build",
        "test": "tsm test/setupFixtures.ts && jest test/getDirs.test.ts normalizeRegex.test.ts",
        "test-vscode": "tsc -p tsconfig.test-vscode.json && node out-test/runTests.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@types/fs-extra": "^9.0.13",
        "@types/glob": "^7.2.0",
        "@types/jest": "^27.0.2",
        "@types/lodash": "^4.14.176",
        "@types/mocha": "^9.0.0",
        "@types/node": "16.11.7",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/vscode": "^1.62.0",
        "@vscode/test-electron": "^1.6.2",
        "babel-jest": "^27.3.1",
        "delay": "^5.0.0",
        "esbuild": "^0.13.13",
        "eslint": "^7.32.0",
        "eslint-plugin-zardoy-config": "^0.1.2",
        "fs-extra": "^10.0.0",
        "glob": "^7.2.0",
        "jest": "^27.3.1",
        "mocha": "^9.1.3",
        "modify-json-file": "^1.2.1",
        "ts-jest": "^27.0.7",
        "tsm": "^2.1.4",
        "type-fest": "^2.5.3",
        "typescript": "^4.4.4",
        "vscode-test": "^1.6.1"
    },
    "dependencies": {
        "@hediet/node-reload": "^0.7.3",
        "@octokit/rest": "^18.12.0",
        "@types/hosted-git-info": "^3.0.2",
        "@types/ini": "^1.3.31",
        "@types/url-join": "^4.0.1",
        "@zardoy/tsconfig": "^1.2.2",
        "esbuild-runner": "^2.2.1",
        "execa": "^5.1.1",
        "find-up": "^6.2.0",
        "git-remote-origin-url": "^3.1.0",
        "github-remote-info": "^1.0.3",
        "hosted-git-info": "^4.0.2",
        "ini": "^2.0.0",
        "is-online": "^9.0.1",
        "lodash": "^4.17.21",
        "nanoid": "^3.1.30",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "read-pkg": "^7.0.0",
        "recursive-last-modified": "^1.0.5",
        "url-join": "^4.0.1",
        "vscode-framework": "0.0.11"
    },
    "license": "MIT"
}
